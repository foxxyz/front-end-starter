{
  "name": "front-end-starter",
  "version": "1.3.0",
  "description": "Boilerplate front-end project with Vite, Vue 3, and Vue Router",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foxxyz/front-end-starter"
  },
  "author": "Ivo Janssen <hello@ivo.la>",
  "license": "MIT",
  "devDependencies": {
    "@appliedminds/eslint-config": "^1.1.1",
    "eslint": "^8.7.0",
    "eslint-plugin-vue": "^8.3.0",
    "sass": "^1.49.0"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "vite": "^2.7.13",
    "vue": "^3.2.27",
    "vue-router": "^4.0.12"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "@appliedminds",
      "plugin:vue/vue3-strongly-recommended"
    ],
    "globals": {
      "__APP_VERSION__": true
    },
    "rules": {
      "vue/html-indent": [
        "error",
        4
      ],
      "vue/multi-word-component-names": "off"
    }
  }
}
